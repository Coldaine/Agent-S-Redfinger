# Provider selection
VISION_PROVIDER=none   # openai | zai | none
VISION_MODEL=gpt-5-mini  # gpt-5 | gpt-5-mini | gpt-5-nano | glm-4.5v (no '-pro' variants)

# Token limits (GPT-5/o3/o4 models use max_completion_tokens; older models use max_tokens)
VISION_MAX_COMPLETION_TOKENS=300  # For GPT-5/o3/o4 models
MAX_COMPLETION_TOKENS=300         # Alternative name for max_completion_tokens
VISION_MAX_TOKENS=300             # For older models
MAX_TOKENS=300                    # Alternative name for max_tokens

# Model restrictions
VISION_ALLOW_PRO_MODELS=false     # Set to true to allow expensive "pro" variants

# API keys
OPENAI_API_KEY=sk-...
OPENAI_BASE_URL=https://api.openai.com  # Optional: Custom OpenAI-compatible API base URL
ZAI_API_KEY=sk-...
ZAI_BASE_URL=https://api.bigmodel.org   # Optional: Custom ZAI API base URL

# Frame contract (model returns normalized in image space; we don't resize by default)
FRAME_WIDTH=1920
FRAME_HEIGHT=1080
SAFE_DELAY_MS=400

# Browser
BROWSER=chrome
